#Enviar solicitud de inicio de sesión al backend

function sendLoginRequest(username, password) {
  const url = 'URL_DEL_BACKEND/login'; // Reemplaza con la URL adecuada
  
  fetch(url, {
    method: 'POST',
    headers: {
      'Content-Type': 'application/json'
    },
    body: JSON.stringify({ username, password })
  })
  .then(response => response.json())
  .then(data => {
    handleLoginResponse(data);
    
    // Aquí puedes llamar a la función para enviar solicitudes adicionales al backend después de iniciar sesión, si es necesario
    if (data.success) {
      // Por ejemplo, puedes llamar a una función para obtener los datos de bienes raíces una vez que el usuario haya iniciado sesión
      fetchRealEstateData();
    }
  })
  .catch(error => {
    console.error('Error:', error);
  });
}

#Enviar solicitud para obtener datos de bienes raíces al backend

function fetchRealEstateData() {
  const url = 'URL_DEL_BACKEND/real-estate'; // Reemplaza con la URL adecuada
  
  const token = localStorage.getItem('token');
  
  fetch(url, {
    headers: {
      'Authorization': 'Bearer ' + token
    }
  })
  .then(response => response.json())
  .then(data => {
    // Aquí puedes manejar los datos recibidos y actualizar la interfaz de usuario en consecuencia
    // Por ejemplo, puedes mostrar los datos de bienes raíces en una tabla o generar elementos HTML para cada propiedad
    console.log('Datos de bienes raíces:', data);
  })
  .catch(error => {
    console.error('Error:', error);
  });
}
